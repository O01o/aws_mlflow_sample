{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect"  : "Allow",
      "Action"  : [
        "s3:PutObject",
        "s3:GetObject",
        "s3:ListBucket",
        "s3:ListAllMyBuckets"
      ],
      "Resource": [
        "*"
      ]
    },
    {
      "Effect"  : "Allow",
      "Action"  : [
        "ec2:StopInstances",
        "ec2:TerminateInstances"
      ],
      "Resource": "*"
    }
  ]
}